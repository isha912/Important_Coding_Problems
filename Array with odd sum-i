#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t,n;
    cin>>t;
    
    int o,e;
    int a[10000];
    while(t--)
    {
        o=e=0;
        cin>>n;
        
        for(int i=0; i<n; i++)
        {
            cin>>a[i];
            if(a[i]%2==0)
                e++;
            else
                o++;
        }
        if (o & 1 or (o>0 and even>0))
            cout<<"YES"<<endl;
        
        else
            cout<<"NO"<<endl;
    }
    return 0;
}
